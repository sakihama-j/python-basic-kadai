        "def calculate_taxed_price(price,tax_percent):\n",
        "  tax_rate = tax_percent / 100\n",
        "  taxed_price = price + (price * tax_rate)\n",
        "  return taxed_price\n",
        "\n",
        "item_price = 110\n",
        "tax_rate = 10\n",
        "\n",
        "result = calculate_taxed_price(item_price, tax_rate)\n",
        "\n",
        "print(result)"
      ]
